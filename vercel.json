{
  "version": 2,
  "name": "qccareerschool-com",
  "scope": "qccareerschool",
  "build": {
    "env": {
      "NPM_TOKEN": "@npm_token"
    }
  },
  "trailingSlash": false,
  "redirects": [
    { "source": "/free-brochure.php", "destination": "/programs" },
    { "source": "/why-distance-learning.php", "destination": "/admissions" },
    { "source": "/faq.php", "destination": "/" },
    { "source": "/prospective-students.php", "destination": "/programs" },
    { "source": "/find-professionals.php", "destination": "/find-professionals" },
    { "source": "/about-qc", "destination": "/" },
    { "source": "/contact-us.php", "destination": "/contact" },
    { "source": "/online-career-center.php", "destination": "/" },
    { "source": "/catalog", "destination": "/programs" },
    { "source": "/catalog/dowload.php", "destination": "/programs" },
    { "source": "/enroll.php", "destination": "/" },
    { "source": "/design/viewprofile.php", "destination": "/profile/" },
    { "source": "/event/viewprofile.php", "destination": "/profile/" },
    { "source": "/makeup/viewprofile.php", "destination": "/profile/" },
    { "source": "/style/viewprofile.php", "destination": "/profile/" },
    { "source": "/travel/viewprofile.php", "destination": "/profile/" },
    { "source": "/general/viewprofile.php", "destination": "/profile/" },
    { "source": "/signup-([0-9]+).html", "destination": "https://affiliates.qccareerschool.com/recruit.php?ref=$1" },
    { "source": "/([0-9]+)-([a-z]+)-([0-9]+).html", "destination": "https://affiliates.qccareerschool.com/idevads.php?id=$1&ad=$3" },
    { "source": "/([0-9]+)-([a-z]+)-([0-9]+)-([0-9]+).html", "destination": "https://affiliates.qccareerschool.com/idevads.php?id=$1&ad=$3&page=$4" },
    { "source": "/([0-9]+).html", "destination": "https://affiliates.qccareerschool.com/idevaffiliate.php?id=$1" },
    { "source": "/([0-9]+)-([0-9]+).html", "destination": "https://affiliates.qccareerschool.com/idevaffiliate.php?id=$1&page=$2" },
    { "source": "/([0-9]+)-([0-9]+)-([0-9]+).html", "destination": "https://affiliates.qccareerschool.com/idevaffiliate.php?id=$1&page=$2&set=$3" },
    { "source": "/([0-9]+)-([0-9]+)-([0-9]+)-([0-9]+).html", "destination": "https://affiliates.qccareerschool.com/idevaffiliate.php?id=$1&page=$2&set=$3&link=$4" },
    { "source": "/([0-9]+)-([0-9]+)-([0-9]+)-([0-9]+)-([a-zA-Z]+).html", "destination": "https://affiliates.qccareerschool.com/idevaffiliate.php?id=$1&page=$2&set=$3&link=$4&keyword=$5" },
    { "source": "/([0-9]+)-([0-9]+)-([0-9]+)-([0-9]+)-([a-zA-Z]+)-([0-9]+).html", "destination": "https://affiliates.qccareerschool.com/idevaffiliate.php?id=$1&page=$2&set=$3&link=$4&keyword=$5&custom=$6" },
    { "source": "/([0-9]+)-([0-9]+)-([0-9]+)-([0-9]+)-([a-zA-Z]+)-([0-9]+)-([0-9]+).html", "destination": "https://affiliates.qccareerschool.com/idevaffiliate.php?id=$1&page=$2&set=$3&link=$4&keyword=$5&custom=$6&url=$7" }
  ],
  "rewrites": [
    { "source": "/service-worker.js", "destination": "/_next/static/service-worker.js" }
  ]
}